{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/news/:category",
      "destination": "/api/news/[category].js"
    },
    {
      "source": "/api/article",
      "destination": "/api/article.js"
    }
  ]
}
